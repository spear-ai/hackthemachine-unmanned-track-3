{
  "name": "@spear-ai/hackthemachine-unmanned-track-3",
  "version": "1.0.0",
  "description": "HACKtheMACHINE: Unmanned Track 3",
  "scripts": {
    "cdk": "cdk",
    "create-sagemaker-algorithm": "node --experimental-json-modules --experimental-specifier-resolution node --loader ts-node/esm.mjs --no-warnings infrastructure/create-sagemaker-algorithm.ts --extension ts",
    "describe-sagemaker-algorithm": "node --experimental-json-modules --experimental-specifier-resolution node --loader ts-node/esm.mjs --no-warnings infrastructure/describe-sagemaker-algorithm.ts --extension ts",
    "lint": "poetry run flake8",
    "model:generate-maps": "poetry run model/server/Forge.py generate --config=CompetitionRound1",
    "model:train": "poetry run python model/server/Forge.py train --config=CompetitionRound1 --RESTORE=None",
    "test:python": "poetry run pytest"
  },
  "dependencies": {
    "@aws-cdk/core": "1.132.0",
    "@aws-cdk/aws-ec2": "1.132.0",
    "@aws-cdk/aws-iam": "1.132.0",
    "@aws-cdk/aws-sagemaker": "1.132.0",
    "@aws-cdk/aws-stepfunctions": "1.132.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.132.0",
    "@aws-cdk/aws-s3": "1.132.0",
    "aws-cdk": "1.132.0",
    "aws-sdk": "2.1036.0",
    "husky": "7.0.4",
    "ts-invariant": "0.9.3",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "private": true
}